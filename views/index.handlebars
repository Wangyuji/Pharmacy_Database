<div class="container-fluid">
  <div style="padding: 5px;" class="row blue">
    <div class="col-sm float-left">
      <h3 style="text-align: left;">Pharmacy's location:</h3>
      <h5 style="text-align: left;">Phone: </h5>
      <h5 style="text-align: left;">Fax: </h5>
    </div>
    <div class="col-sm float-right">
      <h6 style="text-align: right;">Date:</h6>
      <h6 style="text-align: right;">RX Completed:</h6>
      <h6 style="text-align: right;">RX Queue:</h6>
      <button id="newRX" class="btn dark-green float-right">New RX</button>
    </div>
  </div>
    <div style="padding-top: 30px;" class="container-fluid">
      <form action="submit" id="insert_pharmacy_form">
        <legend>Add New Pharmacy</legend>
        <div class="input-group">
          <div class="input-group-prepend">
              <span class="input-group-text dark-green">Pharmacy</span>
          </div>
          <input id="add_name" type="text" class="form-control" placeholder="Fred Meyer">
          <div class="input-group-prepend">
              <span class="input-group-text dark-green">Number</span>
          </div>
          <input id="add_phone" type="text" class="form-control" placeholder="8880000000">
          <div class="input-group-prepend">
              <span class="input-group-text dark-green">Fax</span>
          </div>
          <input id="add_fax" type="text" class="form-control" placeholder="8880000000">
          <div class="input-group-prepend">
              <span class="input-group-text dark-green">Address</span>
          </div>
          <input id="add_address" type="text" class="form-control" placeholder="100th st">
          <div class="input-group-prepend">
              <span id="dea" class="input-group-text dark-green">DEA</span>
          </div>
          <input id="add_dea" type="text" class="form-control" placeholder="0000000000">
          <button type="submit" id="add_pharmacy_btn" class="btn dark-green">Add</button>
        </div>
      </form>
      <form action="submit" id="search_pharmacy_form">
      <legend>Lookup Pharmacy</legend>
        <div class="input-group">
          <div class="input-group-prepend">
              <span class="input-group-text dark-green">Pharmacy</span>
          </div>
          <input id="search_name" type="text" class="form-control" placeholder="Fred Meyer">

          <div class="input-group-prepend">
              <span class="input-group-text dark-green">Number</span>
          </div>
          <input id="search_phone" type="text" class="form-control" placeholder="8880000000">

          <div class="input-group-prepend">
              <span class="input-group-text dark-green">Fax</span>
          </div>
          <input id="search_fax" type="text" class="form-control" placeholder="8880000000">

          <div class="input-group-prepend">
              <span class="input-group-text dark-green">Address</span>
          </div>
          <input id="search_address" type="text" class="form-control" placeholder="100th st">

          <div class="input-group-prepend">
              <span id="dea" class="input-group-text dark-green">DEA</span>
          </div>
          <input id="search_dea" type="text" class="form-control" placeholder="0000000000">

          <button type="submit" id="search_pharmacy_btn" class="btn dark-green">Search</button>
          <div class="col-1">
            <button id="reset_btn" class="btn dark-green">Reset</button>
          </div>
        </div>
      </form>
      <legend>Pharmacies</legend>
      <div class="table-responsive">
        <table class="table-fixed table-bordered table-hover table-dark" id="pharmacies">
          {{!-- Query information gets inserted here --}}
        </table>
      </div>
    </div>
</div>
<script src="./index.js"><{{!}}/script>
